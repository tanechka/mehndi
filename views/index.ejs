			<li><a onclick="filter.setFilter('all')" id='all'>all</a></li>
			<li><a onclick="filter.setFilter('hand')" id='hand'>on the hand</a></li>
			<li><a onclick="filter.setFilter('foot')" id='foot'>on the foot</a></li>
			<li><a onclick="filter.setFilter('back')" id='back'>on the back</a></li>
			<li><a onclick="filter.setFilter('neck')" id='neck'>on the neck</a></li>
			<li><a onclick="filter.setFilter('stomach')" id='stomach'>on the tummy</a></li>
		</ul>
	</div>
	<div id="portfolio-items" class="portfolio-items row wrap">
	<% portfolioList.forEach(function (data){ %>
		<article class="<%= data.category%>">
			<a href="">
				<img src="<%= data.path%>" alt="">
				<div class="overlay">
					<h3><%= data.name %></h3>
					<span><%= data.size%>:<%= data.price%></span>
					<span><%= data.category%></span>
				</div>
			</a>
		</article>
	<% }); %>
</div>
<div id="pageNavPosition"></div>
